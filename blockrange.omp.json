{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "#ffcc88",
            "foreground": "#222222",
            "leading_diamond": "\ue0ba",
            "trailing_diamond": "\ue0bc",
            "style": "diamond",
            "template": "\uf0e7",
            "type": "root"
          },
          {
            "background": "#222222",
            "foreground": "#666666",
            "leading_diamond": "\ue0ba",
            "style": "diamond",
            "template": " {{ if .WSL }}{{ end }}{{.Icon}}",
            "type": "os"
          },
          {
            "background": "#222222",
            "foreground": "#666666",
            "trailing_diamond": "\ue0bc",
            "style": "diamond",
            "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
            "type": "session"
          },
          {
            "background": "#880000",
            "foreground": "#ff8888",
            "leading_diamond": "\ue0ba",
            "trailing_diamond": "\ue0bc",
            "style": "diamond",
            "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
            "type": "status"
          },
          {
            "background": "#f14e32",
            "foreground": "#f0efe7",
            "properties": {
              "branch_icon": "\ue725 ",
              "cherry_pick_icon": "\ue29b ",
              "commit_icon": "\uf417 ",
              "fetch_status": false,
              "fetch_upstream_icon": false,
              "merge_icon": "\ue727 ",
              "no_commits_icon": "\uf0c3 ",
              "rebase_icon": "\ue728 ",
              "revert_icon": "\uf0e2 ",
              "tag_icon": "\uf412 "
            },
            "leading_diamond": "\ue0ba",
            "trailing_diamond": "\ue0bc",
            "style": "diamond",
            "template": " {{ .HEAD }} ",
            "type": "git"
          },
          {
            
            "foreground": "#8cff6c",
            "properties": {
              "fetch_version": true
            },
            "style": "plain",
            "template": "/ \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} /",
            "type": "python"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "foreground": "#007ACC",
            "properties": {
              "style": "austin",
              "threshold": 321
            },
            "style": "plain",
            "template": "{{ .FormattedMs }} /",
            "type": "executiontime"
          },
          {
            "type": "battery",
            "style": "plain",
            "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}",
            "properties": {
            "discharging_icon": "󱟤 ",
            "charging_icon": "󱟦 ",
            "charged_icon": "󱟢 "
            }  
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "style": "diamond",
            "template": "  \ue266 {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
            "type": "sysinfo"
          },
          {
            "properties": {
              "time_format": "Monday 3:04 pm"
            },
            "style": "diamond",
            "template": "<#f14e32> / </>{{ .CurrentDate | date .Format }}<#cccccc> \uf073",
            "type": "time"
          },
          {
            "foreground": "#666666",
            "properties": {
              "folder_icon": "\u2026",
              "style": "mixed"
            },
            "style": "plain",
            "template": "<#f14e32> // </>{{ .Path }} ",
            "type": "path"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#f14e32",
            "style": "plain",
            "template": "{{ .Name }} ",
            "type": "shell",
            "properties": {
              "mapped_shell_names": {
                "pwsh": "\u276f",
                "shell": "\u276f",
                "cmd": ">",
                "lua": ">",
                "nu": ":)",
                "fish": "~>",
                "zsh": "%",
                "bash": ""
              }
            }
          }
        ],
        "type": "prompt"
      }
    ],
    "version": 2
  }
  