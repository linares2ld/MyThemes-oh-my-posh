{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\uf0e7",
          "foreground": "#222222",
          "background": "#ffcc88",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "template": " {{ if .WSL }}{{ end }}{{.Icon}}",
          "foreground": "#666666",
          "background": "#222222",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0bc",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
          "foreground": "#666666",
          "background": "#222222",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " <f>\ue23a</f> {{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "foreground": "#ff8888",
          "background": "#880000",
          "type": "status",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " {{ .HEAD }} {{ if .Upstream }}<#666666>\uebcb</> <#ffffff>{{ .UpstreamIcon }} {{ end }}</>",
          "foreground": "#ffea2f",
          "background": "#222222",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true,
            "folder_name_fallback": false
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }} ",
          "foreground": "#8cff6c",
          "background": "#222222",
          "type": "python",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 321
          },
          "template": "{{ .FormattedMs }} /",
          "foreground": "#007ACC",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\udb85\udfe2 ",
            "charging_icon": "\udb85\udfe6 ",
            "discharging_icon": "\udb85\udfe4 "
          },
          "foreground_templates": [
            "{{if le .Percentage 30}}#ff0000{{end}}",
            "{{if and (gt .Percentage 30) (le .Percentage 50)}}#ffeb3b{{end}}",
            "{{if and (gt .Percentage 50) (lt .Percentage 90)}}#cccccc{{end}}",
            "{{if ge .Percentage 90}}#4caf50{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uf295",
          "type": "battery",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "  \ue266 {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
          "foreground": "#777777",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "03:04:05 pm"
          },
          "template": "<#f14e32> / </>{{ .CurrentDate | date .Format }}<#f14e32> // </>",
          "foreground": "#bbbbbb",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\u2026",
            "folder_separator_icon": "/",
            "style": "mixed"
          },
          "template": "{{ .Path }} ",
          "foreground": "#777777",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\uf101",
              "cmd": "\ueab6",
              "fish": "~>",
              "lua": "\ueab6",
              "nu": ":)",
              "pwsh": "❯",
              "shell": "❯",
              "zsh": "%"
            }
          },
          "template": "{{ .Name }} ",
          "foreground": "#f14e32",
          "type": "shell",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
